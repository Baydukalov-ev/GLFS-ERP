<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Серия</dataPath>
			<field>Серия</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Серия</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Статус</dataPath>
			<field>Статус</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Статус</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НачалоДиапазона</dataPath>
			<field>НачалоДиапазона</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Начало диапазона</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КонецДиапазона</dataPath>
			<field>КонецДиапазона</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Конец диапазона</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Документ</dataPath>
			<field>Документ</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Документ</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Количество</dataPath>
			<field>Количество</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СерияГотовойПродукции</dataPath>
			<field>СерияГотовойПродукции</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Серия готовой продукции</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Данные.Ссылка КАК Серия,
	Данные.бг_ПолныйНачальныйНомерДиапазона - (ВЫРАЗИТЬ(Данные.бг_ПолныйНачальныйНомерДиапазона / &amp;КороткаяСерия - 0.5 КАК ЧИСЛО(14, 0))) * &amp;КороткаяСерия КАК НачалоДиапазона,
	Данные.бг_ПолныйКонечныйНомерДиапазона - (ВЫРАЗИТЬ(Данные.бг_ПолныйКонечныйНомерДиапазона / &amp;КороткаяСерия - 0.5 КАК ЧИСЛО(14, 0))) * &amp;КороткаяСерия КАК КонецДиапазона,
	Данные.бг_ПолныйНачальныйНомерДиапазона КАК НачалоДиапазонаПолное,
	Данные.бг_ПолныйКонечныйНомерДиапазона КАК КонецДиапазонаПолное
ПОМЕСТИТЬ втСерии
ИЗ
	Справочник.СерииНоменклатуры КАК Данные
ГДЕ
	НЕ &amp;ОтборПоРегистратору
	И НЕ Данные.бг_ПолныйНачальныйНомерДиапазона = 0
	И НЕ Данные.бг_ПолныйКонечныйНомерДиапазона = 0
	И ВЫБОР
			КОГДА &amp;ЕстьОтборПоСериям
				ТОГДА Данные.Ссылка В (&amp;МассивСерийФСМ)
			ИНАЧЕ ИСТИНА
		КОНЕЦ
	И ВЫБОР
			КОГДА &amp;ЕстьОтборПоЗаявлениям
				ТОГДА Данные.бг_ЗаявлениеОВыдачеФСМ В (&amp;МассивЗаявлений)
			ИНАЧЕ ИСТИНА
		КОНЕЦ
	И ВЫБОР
			КОГДА &amp;ЕстьОтборПоНакладным
				ТОГДА Данные.бг_НакладнаяНаВыдачуФСМ В (&amp;МассивНакладных)
			ИНАЧЕ ИСТИНА
		КОНЕЦ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ИдентификаторыМарок.СерияФСМ,
	ИдентификаторыМарок.СерияФСМ.бг_ПолныйНачальныйНомерДиапазона - (ВЫРАЗИТЬ(ИдентификаторыМарок.СерияФСМ.бг_ПолныйНачальныйНомерДиапазона / &amp;КороткаяСерия - 0.5 КАК ЧИСЛО(14, 0))) * &amp;КороткаяСерия,
	ИдентификаторыМарок.СерияФСМ.бг_ПолныйКонечныйНомерДиапазона - (ВЫРАЗИТЬ(ИдентификаторыМарок.СерияФСМ.бг_ПолныйКонечныйНомерДиапазона / &amp;КороткаяСерия - 0.5 КАК ЧИСЛО(14, 0))) * &amp;КороткаяСерия,
	ИдентификаторыМарок.СерияФСМ.бг_ПолныйНачальныйНомерДиапазона,
	ИдентификаторыМарок.СерияФСМ.бг_ПолныйКонечныйНомерДиапазона
ИЗ
	РегистрСведений.бг_ДвижениеМарок КАК ДвижениеМарок
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.бг_ИдентификаторыМарок КАК ИдентификаторыМарок
		ПО (&amp;ОтборПоРегистратору)
			И (ДвижениеМарок.Регистратор В (&amp;Регистратор))
			И ДвижениеМарок.ГУИДМарки = ИдентификаторыМарок.ГУИДМарки

ИНДЕКСИРОВАТЬ ПО
	Серия
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	бг_ИдентификаторыМарок.СерияФСМ КАК Серия,
	МАКСИМУМ(бг_ИдентификаторыМарок.ГУИДМарки) КАК ГУИДМарки,
	бг_ИдентификаторыМарок.НомерМарки - (ВЫРАЗИТЬ(бг_ИдентификаторыМарок.НомерМарки / &amp;КороткаяСерия - 0.5 КАК ЧИСЛО(14, 0))) * &amp;КороткаяСерия КАК НомерМарки
ПОМЕСТИТЬ втИдентификаторы
ИЗ
	втСерии КАК втСерии
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.бг_ИдентификаторыМарок КАК бг_ИдентификаторыМарок
		ПО (НЕ &amp;ОтборПоРегистратору)
			И втСерии.Серия = бг_ИдентификаторыМарок.СерияФСМ
{ГДЕ
	(бг_ИдентификаторыМарок.Организация = &amp;Организация)}

СГРУППИРОВАТЬ ПО
	бг_ИдентификаторыМарок.СерияФСМ,
	бг_ИдентификаторыМарок.НомерМарки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втИдентификаторы.Серия КАК Серия,
	втИдентификаторы.НомерМарки КАК НомерМарки,
	Данные.СтатусМарки КАК Статус,
	Данные.Регистратор КАК Документ,
	НЕОПРЕДЕЛЕНО КАК СерияГотовойПродукции
ПОМЕСТИТЬ втМарки
ИЗ
	РегистрСведений.бг_ДвижениеМарок.СрезПоследних(
			&amp;Дата1,
			ГУИДМарки В
				(ВЫБРАТЬ
					втИдентификаторы.ГУИДМарки
				ИЗ
					втИдентификаторы КАК втИдентификаторы)) КАК Данные
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ втИдентификаторы КАК втИдентификаторы
		ПО Данные.ГУИДМарки = втИдентификаторы.ГУИДМарки

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ИдентификаторыМарок.СерияФСМ,
	ИдентификаторыМарок.НомерМарки - (ВЫРАЗИТЬ(ИдентификаторыМарок.НомерМарки / &amp;КороткаяСерия - 0.5 КАК ЧИСЛО(14, 0))) * &amp;КороткаяСерия,
	ДвижениеМарок.СтатусМарки,
	ДвижениеМарок.Регистратор,
	ДвижениеМарок.Серия
ИЗ
	РегистрСведений.бг_ДвижениеМарок КАК ДвижениеМарок
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.бг_ИдентификаторыМарок КАК ИдентификаторыМарок
		ПО (&amp;ОтборПоРегистратору)
			И (ДвижениеМарок.Регистратор В (&amp;Регистратор))
			И ДвижениеМарок.ГУИДМарки = ИдентификаторыМарок.ГУИДМарки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫРАЗИТЬ(втМарки.НомерМарки / 2 КАК ЧИСЛО(8, 0)) КАК НомерДиапазона,
	втМарки.НомерМарки КАК НачалоДиапазона,
	втМарки.НомерМарки КАК КонецДиапазона,
	втМарки.Серия КАК Серия,
	втМарки.СерияГотовойПродукции КАК СерияГотовойПродукции,
	втМарки.Статус КАК Статус,
	втМарки.Документ КАК Документ
ПОМЕСТИТЬ втИсходныеДанные
ИЗ
	втМарки КАК втМарки

ИНДЕКСИРОВАТЬ ПО
	Серия,
	Статус,
	Документ,
	НомерДиапазона,
	НачалоДиапазона,
	КонецДиапазона
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВЫРАЗИТЬ(втИсходныеДанныеСлева.НомерДиапазона / 2 КАК ЧИСЛО(8, 0)) КАК НомерДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.НачалоДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСправа.НачалоДиапазона
		ИНАЧЕ втИсходныеДанныеСлева.НачалоДиапазона
	КОНЕЦ КАК НачалоДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.КонецДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		ИНАЧЕ втИсходныеДанныеСправа.КонецДиапазона
	КОНЕЦ КАК КонецДиапазона,
	втИсходныеДанныеСлева.Статус КАК Статус,
	втИсходныеДанныеСлева.Серия КАК Серия,
	втИсходныеДанныеСлева.СерияГотовойПродукции КАК СерияГотовойПродукции,
	втИсходныеДанныеСлева.Документ КАК Документ
ПОМЕСТИТЬ втИсходныеДанные1
ИЗ
	втИсходныеДанные КАК втИсходныеДанныеСлева
		ЛЕВОЕ СОЕДИНЕНИЕ втИсходныеДанные КАК втИсходныеДанныеСправа
		ПО втИсходныеДанныеСлева.НомерДиапазона = втИсходныеДанныеСправа.НомерДиапазона
			И втИсходныеДанныеСлева.Статус = втИсходныеДанныеСправа.Статус
			И втИсходныеДанныеСлева.Серия = втИсходныеДанныеСправа.Серия
			И втИсходныеДанныеСлева.Документ = втИсходныеДанныеСправа.Документ
			И (втИсходныеДанныеСлева.КонецДиапазона + 1 = втИсходныеДанныеСправа.НачалоДиапазона
				ИЛИ втИсходныеДанныеСлева.НачалоДиапазона = втИсходныеДанныеСправа.КонецДиапазона + 1)

ИНДЕКСИРОВАТЬ ПО
	Серия,
	Статус,
	Документ,
	НомерДиапазона,
	НачалоДиапазона,
	КонецДиапазона
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВЫРАЗИТЬ(втИсходныеДанныеСлева.НомерДиапазона / 2 КАК ЧИСЛО(8, 0)) КАК НомерДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.НачалоДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСправа.НачалоДиапазона
		ИНАЧЕ втИсходныеДанныеСлева.НачалоДиапазона
	КОНЕЦ КАК НачалоДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.КонецДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		ИНАЧЕ втИсходныеДанныеСправа.КонецДиапазона
	КОНЕЦ КАК КонецДиапазона,
	втИсходныеДанныеСлева.Статус КАК Статус,
	втИсходныеДанныеСлева.Серия КАК Серия,
	втИсходныеДанныеСлева.СерияГотовойПродукции КАК СерияГотовойПродукции,
	втИсходныеДанныеСлева.Документ КАК Документ
ПОМЕСТИТЬ втИсходныеДанные2
ИЗ
	втИсходныеДанные1 КАК втИсходныеДанныеСлева
		ЛЕВОЕ СОЕДИНЕНИЕ втИсходныеДанные1 КАК втИсходныеДанныеСправа
		ПО втИсходныеДанныеСлева.НомерДиапазона = втИсходныеДанныеСправа.НомерДиапазона
			И втИсходныеДанныеСлева.Статус = втИсходныеДанныеСправа.Статус
			И втИсходныеДанныеСлева.Серия = втИсходныеДанныеСправа.Серия
			И втИсходныеДанныеСлева.Документ = втИсходныеДанныеСправа.Документ
			И (втИсходныеДанныеСлева.КонецДиапазона + 1 = втИсходныеДанныеСправа.НачалоДиапазона
				ИЛИ втИсходныеДанныеСлева.НачалоДиапазона = втИсходныеДанныеСправа.КонецДиапазона + 1)

ИНДЕКСИРОВАТЬ ПО
	Серия,
	Статус,
	Документ,
	НомерДиапазона,
	НачалоДиапазона,
	КонецДиапазона
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВЫРАЗИТЬ(втИсходныеДанныеСлева.НомерДиапазона / 2 КАК ЧИСЛО(8, 0)) КАК НомерДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.НачалоДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСправа.НачалоДиапазона
		ИНАЧЕ втИсходныеДанныеСлева.НачалоДиапазона
	КОНЕЦ КАК НачалоДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.КонецДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		ИНАЧЕ втИсходныеДанныеСправа.КонецДиапазона
	КОНЕЦ КАК КонецДиапазона,
	втИсходныеДанныеСлева.Статус КАК Статус,
	втИсходныеДанныеСлева.Серия КАК Серия,
	втИсходныеДанныеСлева.СерияГотовойПродукции КАК СерияГотовойПродукции,
	втИсходныеДанныеСлева.Документ КАК Документ
ПОМЕСТИТЬ втИсходныеДанные3
ИЗ
	втИсходныеДанные2 КАК втИсходныеДанныеСлева
		ЛЕВОЕ СОЕДИНЕНИЕ втИсходныеДанные2 КАК втИсходныеДанныеСправа
		ПО втИсходныеДанныеСлева.НомерДиапазона = втИсходныеДанныеСправа.НомерДиапазона
			И втИсходныеДанныеСлева.Статус = втИсходныеДанныеСправа.Статус
			И втИсходныеДанныеСлева.Серия = втИсходныеДанныеСправа.Серия
			И втИсходныеДанныеСлева.Документ = втИсходныеДанныеСправа.Документ
			И (втИсходныеДанныеСлева.КонецДиапазона + 1 = втИсходныеДанныеСправа.НачалоДиапазона
				ИЛИ втИсходныеДанныеСлева.НачалоДиапазона = втИсходныеДанныеСправа.КонецДиапазона + 1)

ИНДЕКСИРОВАТЬ ПО
	Серия,
	Статус,
	Документ,
	НомерДиапазона,
	НачалоДиапазона,
	КонецДиапазона
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВЫРАЗИТЬ(втИсходныеДанныеСлева.НомерДиапазона / 2 КАК ЧИСЛО(8, 0)) КАК НомерДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.НачалоДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСправа.НачалоДиапазона
		ИНАЧЕ втИсходныеДанныеСлева.НачалоДиапазона
	КОНЕЦ КАК НачалоДиапазона,
	ВЫБОР
		КОГДА втИсходныеДанныеСправа.КонецДиапазона ЕСТЬ NULL
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		КОГДА втИсходныеДанныеСправа.НачалоДиапазона &lt; втИсходныеДанныеСлева.НачалоДиапазона
			ТОГДА втИсходныеДанныеСлева.КонецДиапазона
		ИНАЧЕ втИсходныеДанныеСправа.КонецДиапазона
	КОНЕЦ КАК КонецДиапазона,
	втИсходныеДанныеСлева.Статус КАК Статус,
	втИсходныеДанныеСлева.Серия КАК Серия,
	втИсходныеДанныеСлева.СерияГотовойПродукции КАК СерияГотовойПродукции,
	втИсходныеДанныеСлева.Документ КАК Документ
ПОМЕСТИТЬ втИсходныеДанные4
ИЗ
	втИсходныеДанные3 КАК втИсходныеДанныеСлева
		ЛЕВОЕ СОЕДИНЕНИЕ втИсходныеДанные3 КАК втИсходныеДанныеСправа
		ПО втИсходныеДанныеСлева.НомерДиапазона = втИсходныеДанныеСправа.НомерДиапазона
			И втИсходныеДанныеСлева.Статус = втИсходныеДанныеСправа.Статус
			И втИсходныеДанныеСлева.Серия = втИсходныеДанныеСправа.Серия
			И втИсходныеДанныеСлева.Документ = втИсходныеДанныеСправа.Документ
			И (втИсходныеДанныеСлева.КонецДиапазона + 1 = втИсходныеДанныеСправа.НачалоДиапазона
				ИЛИ втИсходныеДанныеСлева.НачалоДиапазона = втИсходныеДанныеСправа.КонецДиапазона + 1)

ИНДЕКСИРОВАТЬ ПО
	Серия,
	Статус,
	Документ,
	НомерДиапазона,
	НачалоДиапазона,
	КонецДиапазона
;

