////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	МаршрутныеЛисты.Организация КАК Организация,
	МаршрутныеЛисты.МаршрутныйЛист КАК МаршрутныйЛист,
	МаршрутныеЛисты.МаршрутныйЛист КАК МЛ,
	МаршрутныеЛисты.Перевозчик КАК Перевозчик,
	МаршрутныеЛисты.Субподрядчик КАК Субподрядчик,
	МаршрутныеЛисты.ФактическийПеревозчик КАК ФактическийПеревозчик,
	МаршрутныеЛисты.Автомобиль КАК Автомобиль,
	МаршрутныеЛисты.Водитель КАК Водитель,
	МаршрутныеЛисты.Заказ КАК Заказ,
	МаршрутныеЛисты.ПунктНазначения КАК ПунктНазначения,
	МаршрутныеЛисты.Контрагент КАК Контрагент,
	ЕСТЬNULL(СуммыЗаказов.СуммаБезНДС, 0) КАК СуммаБезНДС,
	ЕСТЬNULL(ДанныеЗаказов.ОбъемДАЛ, 0) КАК ОбъемДАЛ,
	ЕСТЬNULL(ДанныеЗаказов.Вес, 0) КАК Вес,
	ЕСТЬNULL(ДанныеЗаказов.КоличествоМест, 0) КАК КоличествоМест,
	ЕСТЬNULL(Накладные.Накладная, НЕОПРЕДЕЛЕНО) КАК Накладная,
	ЕСТЬNULL(Накладные.ТТНЕГАИС, НЕОПРЕДЕЛЕНО) КАК ТТН,
	ЕСТЬNULL(Накладные.ТТНЕГАИС.ИдентификаторЕГАИС, НЕОПРЕДЕЛЕНО) КАК ИдентификаторЕГАИС,
	Накладные.НомерНакладной КАК НомерНакладной,
	ЕСТЬNULL(Накладные.СтатусЕГАИС, НЕОПРЕДЕЛЕНО) КАК СтатусЕГАИС,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(МаршрутныеЛисты.Заказ) = ТИП(Документ.ЗаказКлиента)
			ТОГДА ВЫРАЗИТЬ(МаршрутныеЛисты.Заказ КАК Документ.ЗаказКлиента).Комментарий
		КОГДА ТИПЗНАЧЕНИЯ(МаршрутныеЛисты.Заказ) = ТИП(Документ.ЗаказПоставщику)
			ТОГДА ВЫРАЗИТЬ(МаршрутныеЛисты.Заказ КАК Документ.ЗаказПоставщику).Комментарий
		КОГДА ТИПЗНАЧЕНИЯ(МаршрутныеЛисты.Заказ) = ТИП(Документ.ЗаказНаПеремещение)
			ТОГДА ВЫРАЗИТЬ(МаршрутныеЛисты.Заказ КАК Документ.ЗаказНаПеремещение).Комментарий
	КОНЕЦ КАК Комментарий,
	МаршрутныеЛисты.НомерАвтомобиля КАК НомерАвтомобиля,
	МаршрутныеЛисты.Прицеп КАК Прицеп,
	МаршрутныеЛисты.ДатаОтгрузки КАК ДатаОтгрузки,
	&amp;ИнформацияДляВодителей КАК ИнформацияДляВодителей
ИЗ
	МаршрутныеЛисты КАК МаршрутныеЛисты
		ЛЕВОЕ СОЕДИНЕНИЕ ДанныеЗаказов КАК ДанныеЗаказов
		ПО МаршрутныеЛисты.Заказ = ДанныеЗаказов.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ СуммыЗаказов КАК СуммыЗаказов
		ПО МаршрутныеЛисты.Заказ = СуммыЗаказов.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ Накладные КАК Накладные
		ПО МаршрутныеЛисты.Заказ = Накладные.Заказ</query>
	</dataSet>
	<totalField>
		<dataPath>ОбъемДАЛ</dataPath>
		<expression>Сумма(ОбъемДАЛ)</expression>
	</totalField>
	<totalField>
